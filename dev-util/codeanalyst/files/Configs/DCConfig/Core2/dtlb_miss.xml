<?xml version="1.0"?>
<!DOCTYPE dc_configuration SYSTEM "dcconfig.dtd">

<!--     
         Analyze L1 data access and DTLB behavior using event-based profiling
         Date:    11 June 2007
         Version: 1.1c
         Processor: Core2
         Copyright (c) 2007 Advanced Micro Devices, Inc. All rights reserved.
 -->

<dc_configuration>

  <ebp name="DTLB misses" mux_period="1">

    <group>
      <event select="40" mask="0F" os="T" user="T" count="250000"></event>
      <event select="08" mask="01" os="T" user="T" count="25000"></event>
    </group>

    <tool_tip>
      Analyze data access and DTLB behavior
    </tool_tip>

    <description>
      Use this configuration to find data access operations
      with poor L1 data cache locality and poor DTLB behavior. 
    </description>

  </ebp>

</dc_configuration>
